<!DOCTYPE html>

<html lang="en">
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title - ASP Inventory System</title>
    <link href="https://fonts.googleapis.com/css2?family=Noto+Sans+Thai&family=Poppins&display=swap" rel="stylesheet">
    @Styles.Render("~/Content/css")
</head>
<body>

    @{ Html.RenderPartial("../Shared/frontend/navbar"); }

    @RenderBody()

    @{ Html.RenderPartial("../Shared/frontend/footer"); }

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/popper")
    @RenderSection("scripts", required: false)
</body>
</html>
