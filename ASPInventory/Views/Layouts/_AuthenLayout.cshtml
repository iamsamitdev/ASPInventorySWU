<!DOCTYPE html>

<html lang="en">
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title - ASP Inventory System</title>

    <!-- Custom fonts for this template-->
    <link href="https://fonts.googleapis.com/css2?family=Noto+Sans+Thai&family=Nunito+Sans&family=Poppins&display=swap" rel="stylesheet">

    <!-- Custom styles for this template-->
    @Styles.Render("~/Content/backendcss")

</head>
<body class="bg-gradient-primary">

    <div class="container">
        <div class="row justify-content-center">
            <div class="col-xl-10 col-lg-12 col-md-9">
                @RenderBody()
            </div>
        </div>
    </div>

    @Scripts.Render("~/bundles/backendjs")
</body>
</html>
